{% extends "base.html.twig" %}

{% block title %} Download {% endblock %}

{% block body %}

<div class="mt-5">
        
        {% for file in files %}

            <h3>{{ file.fileName}} ({{ file.fileSize}})</h3>

             <a download href="{{ vich_uploader_asset (file) }}">{{ file.fileName}} </a>
             
        {% endfor %}
</div>
{% endblock %}