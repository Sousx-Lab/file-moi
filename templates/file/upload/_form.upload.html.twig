{% if error %}
	<div class="alert alert-danger">{{ error.messageKey | trans(error.messageData, 'security') }}</div>
{% endif %}

{# <form  method="POST" name="upload_request" enctype="multipart/form-data"> #}
{{ form_start (form) }}
	<div class="mb-3">
		{{ form_label(form.uploadedFile, 'Upload file :')}}

		{{ form_widget(form.uploadedFile, {
            'attr': {
            'class': 'form-control' ~ (form.uploadedFile.vars.valid ? ' ' : ' is-invalid'),
            }}) 
		}}
		<div class="invalid-feedback  mb-3">

			{{ form_errors(form.uploadedFile) }}

		</div>
	</div>

	    <button type="submit" class="btn btn-primary btn-lg">Upload</button>

	{{ form_widget(form._token)}}
{{ form_end (form) }}
{# </form> #}
